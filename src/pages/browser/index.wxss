page {
  width: 100%;
  height: auto;
  position: relative;
  -webkit-overflow-scrolling: touch;
}
.browser {
  position: relative;
  width: 100%;
  height: auto;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.browser .showPicMask {
  z-index: 8;
  width: 100%;
  height: 100%;
  position: fixed;
  background: rgba(255, 255, 255, 0.295);
  backdrop-filter: blur(8px) brightness(50%);
  justify-content: center;
  display: flex;
}
.browser .showPicMask .maskContent {
  display: flex;
  position: relative;
  width: 80%;
  height: 60%;
  margin-top: 20%;
  justify-content: center;
  align-items: center;
  z-index: 2;
}
.browser .showPicMask .maskContent .clickArea {
  position: fixed;
  z-index: 3;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.browser .showPicMask .maskContent .maskImg {
  box-shadow: 4rpx 4rpx 8rpx rgba(0, 0, 0, 0.5);
  border-radius: 2%;
  width: 100%;
  height: 100%;
  z-index: 4;
}
.browser .showPicMask .maskContent .maskBtnGrp {
  display: flex;
  position: fixed;
  width: 80%;
  height: 20%;
  bottom: 0;
  justify-content: center;
  flex-direction: row;
  z-index: 3;
}
.browser .showPicMask .maskContent .maskBtnGrp .maskBtn {
  width: 35%;
  height: 80%;
  display: flex;
  position: relative;
  margin-right: 4%;
  margin-left: 4%;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  z-index: 3;
}
.browser .showPicMask .maskContent .maskBtnGrp .maskBtn .maskBtnImg {
  height: 130rpx;
  width: 130rpx;
  z-index: 5;
}
.browser .showPicMask .maskContent .maskBtnGrp .maskBtn .maskBtnText {
  margin-top: 3%;
  color: white;
  font-weight: bold;
  z-index: 5;
}
.browser .showPicMask .maskContent .maskBtnGrp .maskBtn.maskBtn-hover {
  transform: scale(0.85);
}
.browser .navBar {
  height: auto;
  width: 100%;
  background-color: white;
  position: fixed;
  transition: all 0.2s;
  z-index: 1;
}
.browser .navBar .scroll {
  white-space: nowrap;
  display: flex;
  width: auto;
  height: auto;
  transition: all 0.2s;
  padding-top: 3%;
  z-index: 2;
}
.browser .navBar .scroll .item {
  display: inline-block;
  position: relative;
  margin-left: 6rpx;
  margin-right: 6rpx;
  transition: all 0.2s;
  height: auto;
  width: auto;
  z-index: 3;
}
.browser .navBar .scroll .item .itemImg {
  transition: all 0.2s;
  border-radius: 4%;
  z-index: 4;
  position: relative;
  padding-bottom: 35rpx;
}
.browser .navBar .scroll .item .itemImg .itemImgBorder {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  border: 7px solid #ff3366;
  z-index: 5;
  transition: all 0.2s;
  box-sizing: border-box;
  border-radius: 4%;
  display: flex;
  justify-content: center;
}
.browser .navBar .scroll .item .itemImg .itemImgBorder .itemImgBorderText {
  font-size: 26rpx;
  color: white;
  background-color: #ff3366;
  position: absolute;
  top: 0;
  left: 0;
  padding-left: 4rpx;
  padding-right: 12rpx;
  padding-bottom: 3rpx;
  border-bottom-right-radius: 30%;
}
.browser .navBar .scroll .item .itemImg .itemImgBorder .itemImgBorderTri {
  position: absolute;
  bottom: -25rpx;
  z-index: 6;
  width: 0;
  height: 0;
  border-left: 20px solid transparent;
  border-right: 20px solid transparent;
  border-top: 25px solid #ff3366;
  font-size: 0;
  line-height: 0;
}
.browser .navBar .scroll .item.item-hover {
  transform: scale(0.95);
}
.browser .waterfall {
  width: 100%;
  top: 0;
  display: flex;
}
.browser .waterfall .left-div {
  display: flex;
  flex-direction: column;
  width: 50%;
  height: auto;
  position: relative;
  z-index: 0;
  align-items: center;
}
.browser .waterfall .left-div .card {
  width: 85%;
  margin-top: 3%;
  margin-left: 40rpx;
  border-radius: 6rpx;
}
.browser .waterfall .left-div .card .cardImg {
  border-radius: 6rpx;
  width: 100%;
}
.browser .waterfall .left-div .card.card-hover {
  transform: scale(0.95);
}
.browser .waterfall .right-div {
  display: flex;
  flex-direction: column;
  width: 50%;
  height: auto;
  position: relative;
  z-index: 0;
  align-items: center;
}
.browser .waterfall .right-div .card {
  width: 85%;
  margin-top: 3%;
  margin-right: 40rpx;
  border-radius: 6rpx;
}
.browser .waterfall .right-div .card .cardImg {
  border-radius: 6rpx;
  width: 100%;
}
.browser .waterfall .right-div .card.card-hover {
  transform: scale(0.95);
}
.browser .divider {
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #FF3366;
  margin-bottom: 20%;
  margin-top: 3%;
}
.browser .btnGrp {
  display: flex;
  width: 100%;
  position: fixed;
  bottom: 0;
  padding-top: 3%;
  padding-bottom: 3%;
  background-color: white;
}
.browser .btnGrp .button {
  border-radius: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 36rpx;
  text-align: center;
  transition: all 0.2s;
}
.browser .btnGrp .button.btn-hover {
  transform: scale(0.85);
}
.browser .btnGrp .button.pink {
  width: 50%;
  color: white;
  background: #FF3366;
  margin-left: 3%;
  margin-right: 40rpx;
}
.browser .btnGrp .button.white {
  width: 270rpx;
  border: 1px solid #FF3366;
  color: #FF3366;
  background: white;
  margin-left: 40rpx;
}
