.page-home {
  box-sizing: border-box;
  position: relative;
  background: #fff;
  -webkit-overflow-scrolling: touch;
}
.page-home .main {
  position: relative;
}
.page-home .main .main-bg {
  width: 100%;
  height: 98%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}
.page-home .main .main-container {
  position: relative;
  width: 100%;
  box-sizing: border-box;
  padding: 130rpx 40rpx 40rpx;
}
.page-home .main .main-container.iphoneX {
  padding: 145rpx 40rpx 40rpx;
}
.page-home .main .main-container .category-wrap {
  width: 100%;
  margin-top: 0rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.page-home .main .main-container .bottomInfo {
  font-size: 20rpx;
  color: #cccccc;
  text-align: center;
}
