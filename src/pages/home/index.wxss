.page-home {
  box-sizing: border-box;
  position: relative;
  background: #fff;
  -webkit-overflow-scrolling: touch;
}
.page-home .title-filler {
  width: 100%;
  background-color: red;
}
.page-home .tooltip {
  height: 90rpx;
  width: 100%;
  background-color: orange;
}
.page-home .nav-bar {
  width: 140rpx;
  height: 1500rpx;
  background-color: yellow;
  float: left;
}
